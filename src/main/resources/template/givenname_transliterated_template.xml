<saml2:Attribute 
        FriendlyName="FirstName" Name="http://eidas.europa.eu/attributes/naturalperson/CurrentGivenName" NameFormat="urn:oasis:names:tc:SAML:2.0:attrnameformat:uri">
        <saml2:AttributeValue xsi:type="eidas:CurrentGivenNameType">
            $value
        </saml2:AttributeValue>  
        <saml2:AttributeValue xsi:type="eidas:CurrentGivenNameType" LatinScript="$isLatinScript">
            $transliteratedvalue
        </saml2:AttributeValue>        
    </saml2:Attribute>